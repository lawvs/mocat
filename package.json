{
  "name": "rabbit-mock",
  "version": "0.0.1",
  "description": "mocking for ajax requests",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "tsc && webpack dist/index.js --mode=production --output-library RabbitMock --progress --color",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --fix --ext .ts src",
    "beforepublish": "npm run build && npm run types",
    "clean": "rm -rf dist/ types/",
    "test": "jest",
    "types": "tsc -d --declarationDir types"
  },
  "keywords": [
    "mock",
    "mockAjax"
  ],
  "author": "",
  "dependencies": {
    "fetch-mock": "^7.3.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "xhr-mock": "^2.5.0"
  },
  "devDependencies": {
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  }
}
