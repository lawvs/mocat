{
  "name": "rabbit-mock",
  "version": "0.0.1",
  "private": true,
  "description": "mocking for ajax requests",
  "main": "dist/main.min.js",
  "types": "types/index.d.ts",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --config scripts/webpack.config.js --mode=development --colors --watch --hot",
    "build": "webpack --config scripts/webpack.config.js --mode=production  --progress --color",
    "beforepublish": "npm run build && npm run types",
    "lint": "eslint --ext .ts,.tsx,.json src",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.json src",
    "typecheck": "tsc --noEmit --project .",
    "types": "tsc -d --declarationDir types",
    "clean": "rm -rf dist/ types/",
    "test": "jest"
  },
  "keywords": [
    "mock",
    "mockAjax"
  ],
  "author": "lawvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lawvs/rabbit-mock"
  },
  "bugs": {
    "url": "https://github.com/lawvs/rabbit-mock/issues"
  },
  "homepage": "https://github.com/lawvs/rabbit-mock#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  }
}
