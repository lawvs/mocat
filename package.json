{
  "name": "rabbit-mock",
  "version": "0.0.1",
  "private": true,
  "description": "mocking for ajax requests",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "typecheck": "lerna exec --parallel --ignore eslint-config -- tsc --noEmit --project .",
    "clean": "rm -rf coverage && lerna run --parallel clean",
    "test": "jest",
    "test:ci": "jest --coverage"
  },
  "keywords": [
    "mock",
    "mockAjax"
  ],
  "author": "lawvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lawvs/rabbit-mock"
  },
  "bugs": {
    "url": "https://github.com/lawvs/rabbit-mock/issues"
  },
  "homepage": "https://github.com/lawvs/rabbit-mock#readme",
  "devDependencies": {
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "jest": "^26.1.0",
    "lerna": "^3.22.1",
    "ts-jest": "^26.1.3",
    "typescript": "^4.0.2"
  }
}
